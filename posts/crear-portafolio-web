</head><body id="body"><!-- universe--><canvas id="universe"></canvas><!-- background img--><!-- loading--><!-- console--><div id="console"><div class="close-btn" onclick="sco.hideConsole()"><i class="solitude fas fa-xmark"></i></div><div class="console-card-group"><div class="console-card-group-left"><div class="console-card" id="card-newest-comments" onclick="sco.hideConsole()"><div class="card-content"><div class="author-content-item-tips">Interaction</div><div class="author-content-item-title">Latest comments</div></div><div class="console_recentcomments"><script pjax>(async () => {
    document.querySelector('.console-card .console_recentcomments').textContent = `Loading...`
    const emojiReg = /<img [^>]+ class="wl-emoji">/g
    let cache = utils.saveToLocal.get('waline-recent-comments-console')
    if (cache) {
        setHtml(document.querySelector('.console-card .console_recentcomments'), cache)
        return
    }
    let ls = []
    await fetch('http://comments-blog-jet.vercel.app//api/comment?type=recent&count=6', {method: 'GET'}).then(async res => res.json())
        .then(async data => {
            for (const i of data.data) {
                let title = ''
                if (i.url) {
                    await fetch(i.url).then(res => res.text()).then(html => {
                        const parser = new DOMParser()
                        const doc = parser.parseFromString(html, 'text/html')
                        title = doc.querySelector('title').innerText
                    }).catch(() => {
                        title = i.url
                    })
                }
                if (title.indexOf('|') > 0) {
                    title = title.split('|')[0]
                }
                ls.push({
                    title: title,
                    url: i.url,
                    nick: i.nick,
                    avatar: i.avatar,
                    time: new Date(i.time),
                    content: formatContent(i.comment)
                })
            }
            setHtml(document.querySelector('.console-card .console_recentcomments'), ls)
            utils.saveToLocal.set('waline-recent-comments-console', ls, 0.2)
        })

    function setHtml(el, data) {
        el.innerHTML = data.length !== 0 ? data.map(i => `
                            <div class="comment-card" title="${i.title}" onclick="pjax.loadUrl('${i.url}')">
                                <div class="comment-info">
                                    <img src="${i.avatar}" class="nolazyload" alt="${i.nick}">
                                    <div>
                                        <span class="comment-user">${i.nick}</span>
                                    </div>
                                    <time class="comment-time" datetime="${i.time}"></time>
                                </div>
                                <div class="comment-content">${i.content}</div>
                                <div class="comment-title">
                                <i class="solitude fas fa-comment"></i>
                                ${i.title}</div>
                            </div>
                        `).join('') : `No comments...`
        if (typeof utils !== 'undefined') utils.diffDateFormat(document.querySelectorAll('.comment-time'))
        else {
            document.addEventListener('pjax:complete', () => utils.diffDateFormat(document.querySelectorAll('.comment-time')))
            document.addEventListener('DOMContentLoaded', () => utils.diffDateFormat(document.querySelectorAll('.comment-time')))
        }
    }

    function formatContent(content) {
        content = content.replace(emojiReg, '[Emoji]')
        content = content.replace(/<img.*?>/g, '[Image]');
        content = content.replace(/<a.*?>.*?<\/a>/g, '[Link]');
        content = content.replace(/<pre.*?>.*?<\/pre>/g, '[Code]');
        content = content.replace(/<.*?>/g, '');
        return content
    }
})()</script></div></div></div><div class="console-card-group-right"><div class="console-card tags" onclick="sco.hideConsole()"><div class="card-content"><div class="author-content-item-tips">Tags</div><div class="author-content-item-title">Find interesting areas</div></div><div class="card-tag-cloud"><a href="/blog/tags/Computacion-Cuantica/">Computaci√≥n Cu√°ntica<sup>1</sup></a><a href="/blog/tags/Tecnologia/">Tecnolog√≠a<sup>1</sup></a><a href="/blog/tags/Ciencia/">Ciencia<sup>5</sup></a><a href="/blog/tags/Portfolio/">Portfolio<sup>1</sup></a><a href="/blog/tags/GitHub/">GitHub<sup>2</sup></a><a href="/blog/tags/Desarrollo-Web/">Desarrollo Web<sup>3</sup></a><a href="/blog/tags/Linux/">Linux<sup>2</sup></a><a href="/blog/tags/Distribuciones/">Distribuciones<sup>1</sup></a><a href="/blog/tags/Fisica/">F√≠sica<sup>1</sup></a><a href="/blog/tags/Quimica/">Qu√≠mica<sup>1</sup></a><a href="/blog/tags/Recursos/">Recursos<sup>2</sup></a><a href="/blog/tags/Programacion/">Programaci√≥n<sup>1</sup></a><a href="/blog/tags/Arch-Linux/">Arch Linux<sup>1</sup></a><a href="/blog/tags/Paquetes/">Paquetes<sup>1</sup></a><a href="/blog/tags/Pacman/">Pacman<sup>1</sup></a><a href="/blog/tags/Historia/">Historia<sup>1</sup></a><a href="/blog/tags/Extensiones/">Extensiones<sup>1</sup></a><a href="/blog/tags/Navegadores/">Navegadores<sup>1</sup></a><a href="/blog/tags/Optimizacion-Web/">Optimizaci√≥n Web<sup>1</sup></a><a href="/blog/tags/Tutorial/">Tutorial<sup>1</sup></a><a href="/blog/tags/Diseno-Web/">Dise√±o Web<sup>1</sup></a><a href="/blog/tags/Herramientas/">Herramientas<sup>1</sup></a><a href="/blog/tags/Ingenieria/">Ingenier√≠a<sup>1</sup></a><a href="/blog/tags/Diseno-automotriz/">Dise√±o automotriz<sup>1</sup></a><a href="/blog/tags/Espacio/">Espacio<sup>1</sup></a></div></div><div class="console-card history" onclick="sco.hideConsole()"><ul class="card-archive-list"><li class="item"><a href="/archives/2023/03/"><span class="date">2023/03</span><div class="count-group">